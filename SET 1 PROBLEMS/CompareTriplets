#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int d = 0;
    int e = 0;
    int a[3],b[3],i;
    for(i=0;i<3;i++)
 {
    scanf("%d",&a[i]);
 }
    for(i=0;i<3;i++)
 {
    scanf("%d",&b[i]);
 }
 for(i=0;i<3;i++)
    {
        if(a[i]>b[i])
        {
            d++;
        }
        if(a[i]<b[i])
        {
            e++;
        }
        if(a[i]==b[i])
        {
            printf("");
        }
    }
    printf("%d %d",d,e);

return 0;
}
